        "macros_add": [
            "STM32L452xx",
            "MBED_TICKLESS",
            "EXTRA_IDLE_STACK_REQUIRED",
            "MBED_SPLIT_HEAP"
        ],
        "overrides": {
            "lpticker_delay_ticks": 0
        },
        "detect_code": [
            "1234"
        ],
        "device_has_add": [
            "ANALOGOUT",
            "CRC",
            "SERIAL_ASYNCH",
            "CAN",
            "TRNG",
            "FLASH",
            "MPU"
        ],
        "device_name": "STM32L452RETx",
        "bootloader_supported": true
    }
